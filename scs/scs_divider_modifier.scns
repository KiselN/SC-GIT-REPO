!scs_divider_modifier
<= nrel_identification: 
{
	["sc.s-разделитель модификатор коннектора"] (<- ru_lang);
}
<- non_atomic_partition
<= nrel_superpartition:
	scs_divider
<= nrel_decomposition: 
{
	scs_divider_simple_colon;
	scs_divider_double_colon;
	scs_divider_simple_plus;
	scs_divider_double_plus;
}