!scs_divider
<= nrel_identification: {["Раздел OSTIS. Разделитель SCs-кода"] (<- ru_lang);}
<- non_atomic_partition
<= nrel_superpartition:
	scs_code
=> nrel_previous_partition:
	scs_sentence_5_level
<= nrel_decomposition:
{
	scs_divider_connector;
	scs_divider_incidence;
	scs_divider_modifier;
	scs_divider_semicolon;
	scs_divider_subsentence;
}