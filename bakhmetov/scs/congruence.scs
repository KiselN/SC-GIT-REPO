/!* keyword: nrel_congruence */
nrel_congruence => nrel_main_ru_idtf: 
	[Cоответствие*];;
nrel_congruence => nrel_ru_idtf: 
	[сопоставление*];
	[отношение соответствия*];
	[быть соответствием];;

nrel_congruence <= nrel_definition:
	[
	Множество является соответствием* между двумя множествами тогда и только тогда, когда оно является бинарным отношением и первое множество включает* унарную проекцию* этого бинарного отношения по атрибутивному отношению 1_, а второе множество – унарную проекцию* этого бинарного отношения по атрибутивному отношению 2_.
	] (* <=> nrel_semantic_equivalence: "file://images/congruence.png";; *);;
nrel_congruence <= nrel_inclusion: 
	[понятие];
	[канторовское множество];
	[отношение];;
nrel_congruence <= nrel_explanation:
	[
		•	r – отношение соответствия;<br>
		•	r – бинарное ориентированное отношение с атрибутами 1_ и 2_;<br>
		•	х1 и х2 – пара;<br>
		•	у1 и у2 – пара.
		
	] (* <=> nrel_semantic_equivalence: "file://images/congruenceExample.png" (* <= nrel_comment: [/*в SCg-коде*/];; *);; *);;